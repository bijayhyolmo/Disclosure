
@model IEnumerable<Disclosure.Models.ViewGiftTravelFilings.TravelData>
<h2>
    Search Results
</h2>
<table class="library-table table">
    <thead>
        <tr>
            <th>
                Member Name
            </th>
            <th>
                Filter Name
            </th>
            <th>
                Destination
            </th>
            <th>Travel Dates</th>
            <th>Sponsor</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Any())
        {
            foreach (var result in Model)
            {
                <tr>
                    <td>@result.MemberName</td>
                    <td>@result.FilerName</td>
                    <td>@result.Destination</td>
                    <td>@result.TravelDateFrom - @result.TravelDateTo</td>
                    <td>@result.Sponsor</td>
                </tr>
            }
        }
        else
        {
            <tr><td colspan="5">No Data</td></tr>
        }
    </tbody>
</table>