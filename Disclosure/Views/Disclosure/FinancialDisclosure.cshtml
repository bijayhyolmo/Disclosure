
@{
    ViewBag.Title = "FinancialDisclosure";
}

<div class="session-id-mobile visible-xs">
    <ul>
        <li><svg role="img" aria-label="Yellow Star" xmlns="http://www.w3.org/2000/svg" width="19" height="18" viewBox="8.5 -3.9 19 17.8"><polygon points="27.5 3 20.3 3 18.1 -3.9 15.8 3 8.5 3 14.4 7.1 12.2 13.9 18 9.7 23.9 13.9 21.7 7.1 " fill="#FFD262" /></svg></li>
        @*<li>@Model.eventName</li>*@
        <li><svg role="img" aria-label="Yellow Star" xmlns="http://www.w3.org/2000/svg" width="19" height="18" viewBox="8.5 -3.9 19 17.8"><polygon points="27.5 3 20.3 3 18.1 -3.9 15.8 3 8.5 3 14.4 7.1 12.2 13.9 18 9.7 23.9 13.9 21.7 7.1 " fill="#FFD262" /></svg></li>
    </ul>
</div>

<section id="main-content" class="main-view container double-column about" data-main="about">
    <div class="row equalheights">
        <div class="col-xs-12 col-sm-12 col-md-3 left-nav left-nav-group">
            <ul class="nav nav-pills nav-stacked" data-load-views="true" role="tablist">
                <li><h2 class="library-h2">Reports</h2></li>
                <li role="tab" class="active">
                    <a class="init" href="#Report" data-custom-nav="sidenav" data-view="ViewFinancialDisclosureReports" aria-controls="What are Financial Disclosure Reports" data-toggle="pill" tabindex="0">Overview</a>
                </li>
                <li role="tab" class="active">
                    <a class="init" href="#ForMembersAndStaff" data-custom-nav="sidenav" aria-controls="For Members And Staff" data-toggle="pill" tabindex="0">For Members And Staff</a>
                </li>
                <li role="tab" class="active">
                    <a class="init" href="#ForCongressionalCandidates" data-custom-nav="sidenav" aria-controls="For Congressional Candidates" data-toggle="pill" tabindex="0">For Congressional Candidates</a>
                </li>
                <li role="tab" class="active">
                    <a class="init" href="#ObtainDocuments" data-custom-nav="sidenav" aria-controls="Obtain Documents" data-toggle="pill" tabindex="0">Obtain Documents</a>
                </li>
                <li role="tab">
                    <a href="#Search" data-custom-nav="sidenav" data-view="ViewFinancialDisclosureSearch" aria-controls="Search" data-toggle="pill" tabindex="0">Search</a>
                </li>
            </ul>
        </div>
        <input type="number" id="currentPage" hidden value="@ViewBag.Page" />
        <input type="text" id="currentKeyword" hidden value="@ViewBag.Keyword" />
        <div class="tab-content col-xs-12 col-sm-8 col-md-9">
            <div role="tabpanel" class="tab-pane library-tab_pane" id="Report" tabindex="0">
            </div>
            <div role="tabpanel" class="tab-pane library-tab_pane" id="Search" tabindex="0"></div>
        </div>
    </div>

</section>
<script>
    var MyAppUrlSettings = {
        ViewDisclosureSearch: '@Url.Action("ViewFinancialDisclosureSearch", "Disclosure")',
        ViewReport: '@Url.Action("ViewFinancialDisclosureReports", "Disclosure")'
    };
    document.title = 'Office of the Clerk, U.S. House of Representatives - Financial Disclosure Reports';
</script>

